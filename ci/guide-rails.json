{
	"metadata": {
		"version": 2
	},
	"application": {
		"name": "simple-hello-world-java",
		"language": "JAVA_MAVEN",
		"artifact_store": {
			"type": "s3",
			"repositories": [
				{
					"category": "rc-packaging",
					"bucket": "guide-rails-calculi-rc"
				},
				{
					"category": "final-packaging",
					"bucket": "guide-rails-calculi-final"
				},
				{
					"category": "logs",
					"bucket": "guide-rails-calculi-logs"
				}
			]
		}
	},
	"pipeline": {
		"segments": [
			{
				"name": "Build",
				"segment_type": "build",
				"publish_attributes": {
					"docker_images": [
						{
							"repository": "((application.name))",
							"tags": [
								"((application.version))"
							],
							"context": "."
						},
						{
							"repository": "((application.name))-java",
							"dockerFilePath": "DockerJava",
							"tags": [
								"((application.version))"
							],
							"context": "."
						}
					]
				},
				"exit_actions": {
					"default": {
						"fetch_logs": true,
						"code_analysis": {
							"publish": true,
							"properties": null
						},
						"delete_instances": true
					},
					"on_success": {
						"code_analysis": null
					},
					"on_failure": {
						"code_analysis": null
					},
					"on_abort": {
						"code_analysis": null
					}
				},
				"deployment_strategy": {
					"canary_watch_time": "",
					"update_watch_time": "",
					"serial": false
				},
				"application_init": {},
				"instance_tags": [
					{
						"name": "application.name",
						"value": "((application.name))",
						"when": "0001-01-01T00:00:00Z"
					}
				],
				"post_deployment_actions": {
					"loadbalancer": {}
				},
				"cache_info": {
					"paths": [
						"((maven.repo.local))"
					]
				},
				"tasks": [
					{
						"name": "simple-hello-world-java",
						"steps": [
							{
								"name": "print env",
								"definition": "print env",
								"command": "env | sort",
								"properties": null
							}
						],
						"docker_options": {}
					}
				]
			}
		],
		"exit_actions": {
			"default": {
				"fetch_logs": true,
				"code_analysis": {
					"properties": []
				},
				"delete_instances": true
			},
			"on_success": {
				"code_analysis": null
			},
			"on_failure": {
				"code_analysis": null
			},
			"on_abort": {
				"code_analysis": null
			}
		},
		"default_properties": [
			{
				"name": "gr.veracode.static.scan",
				"value": "false",
				"type": "string"
			},
			{
				"name": "jdk.version",
				"value": "12",
				"type": "string"
			},
			{
				"name": "source_dirs",
				"value": "./",
				"type": "string"
			}
		],
		"deployment_files": [
			{
				"source_path": "ci/bin/start-service.sh",
				"target_path": "bin/start-service.sh"
			},
			{
				"source_path": "ci/bin/integration-test.sh",
				"target_path": "bin/integration-test.sh"
			}
		],
		"application_init": {},
		"process": {
			"init": {
				"command": "bin/start-service.sh"
			}
		}
	},
	"organization": {
		"orgname": "Tutorial"
	}
}